{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/src/frontend/$1"
    },
    {
      "src": "/verificar-email",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/frontend/index.html"
    }
  ]
}
